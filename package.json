{
  "name": "quorum-i18n",
  "version": "1.7.1",
  "description": "Repo for all the i18n resources for the Quorum's Sales Team",
  "author": "quorumsco <https://www.quorum.co>",
  "main": "dist/index.js",
  "scripts": {
    "build:app": "babel src/index.js --out-dir dist",
    "fix:prettier": "prettier --write \"**/*.{json,js}\"",
    "create:language-fallbacks": "node --experimental-json-modules ./createLanguageFallbacks.js",
    "test:manual": "npm run build:app && node --experimental-json-modules __tests__/index.test.js",
    "test": "jest",
    "watch": "jest --no-cache --watch ./*"
  },
  "dependencies": {
    "deep-extend": "^0.6.0",
    "fs-extra": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "babel-jest": "^26.0.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-static-fs": "^3.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-wildcard": "^6.0.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lodash": "^4.17.15",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kissu/quorum-i18n.git"
  },
  "keywords": [
    "quorum"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kissu/quorum-i18n/issues"
  },
  "homepage": "https://github.com/kissu/quorum-i18n#readme"
}
