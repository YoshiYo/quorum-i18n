{
  "name": "quorum-i18n",
  "version": "1.1.5",
  "engines": {
    "node": ">=14.2.0",
    "npm": ">=6.14.4"
  },
  "type": "module",
  "description": "Repo for all the i18n resources for the Quorum's Sales Team",
  "author": "quorumsco <https://www.quorum.co>",
  "scripts": {
    "build:app": "babel index.js --out-dir dist",
    "fix:prettier": "prettier --write \"**/*.json\"",
    "create:language-fallbacks": "node --experimental-json-modules ./createLanguageFallbacks.js"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "deep-extend": "^0.6.0",
    "fs-extra": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kissu/quorum-i18n.git"
  },
  "keywords": [
    "quorum"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kissu/quorum-i18n/issues"
  },
  "homepage": "https://github.com/kissu/quorum-i18n#readme"
}
